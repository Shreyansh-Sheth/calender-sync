# ICS Calendar URL (e.g., Outlook, iCloud)
ICS_URL=https://outlook.office365.com/owa/calendar/xxx/calendar.ics

# Google OAuth Credentials
# Get these from Google Cloud Console
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret

# Google Refresh Token
# Run `bun run get-token` to obtain this
GOOGLE_REFRESH_TOKEN=your-refresh-token

# Sync interval in milliseconds (default: 60000 = 1 minute)
SYNC_INTERVAL_MS=60000

# Optional: Custom calendar name (leave empty for random animal name)
CALENDAR_NAME=
